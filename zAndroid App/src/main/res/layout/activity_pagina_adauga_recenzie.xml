<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/wallpapperfara"
    tools:context=".Pagina_Adauga_Recenzie">

    <TextView
        android:id="@+id/textView11"
        android:layout_width="204dp"
        android:layout_height="43dp"
        android:layout_marginTop="21dp"
        android:text="NumeProfesor"
        android:gravity="center_vertical|center_horizontal"
        app:layout_constraintBottom_toTopOf="@+id/textView15"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="204dp"
        android:layout_height="43dp"
        android:layout_marginBottom="66dp"
        android:text="Materia"
        android:gravity="center_vertical|center_horizontal"
        app:layout_constraintBottom_toTopOf="@+id/ratingBar4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="204dp"
        android:layout_height="43dp"
        android:text="Scoala"
        android:gravity="center_vertical|center_horizontal"
        app:layout_constraintBottom_toTopOf="@+id/textView14"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="120dp"
        android:layout_height="45dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="67dp"
        android:text="Cunoștinte :"
        android:gravity="center_vertical|center_horizontal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        android:layout_marginLeft="8dp" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="120dp"
        android:layout_height="45dp"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="13dp"
        android:text="Corectitudine :"
        android:gravity="center_vertical|center_horizontal"
        app:layout_constraintBottom_toTopOf="@+id/textView27"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginLeft="10dp" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="120dp"
        android:layout_height="45dp"
        android:layout_marginStart="10dp"
        android:text="Mod Evaluare :"
        android:gravity="center_vertical|center_horizontal"
        app:layout_constraintBottom_toTopOf="@+id/textView23"
        app:layout_constraintEnd_toStartOf="@+id/ratingBar7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView25"
        android:layout_marginLeft="10dp" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="120dp"
        android:layout_height="45dp"
        android:layout_marginStart="8dp"
        android:text="Comunicare :"
        android:gravity="center_vertical|center_horizontal"
        app:layout_constraintBottom_toTopOf="@+id/textView24"
        app:layout_constraintEnd_toStartOf="@+id/ratingBar9"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView26"
        android:layout_marginLeft="8dp" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="120dp"
        android:layout_height="45dp"
        android:layout_marginStart="8dp"
        android:text="Mod Predare :"
        android:gravity="center_vertical|center_horizontal"
        app:layout_constraintBottom_toTopOf="@+id/textView25"
        app:layout_constraintEnd_toStartOf="@+id/ratingBar8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView16"
        android:layout_marginLeft="8dp" />

    <RatingBar
        android:id="@+id/ratingBar4"
        android:layout_width="235dp"
        android:layout_height="45dp"
        app:layout_constraintBottom_toTopOf="@+id/ratingBar8"
        app:layout_constraintStart_toEndOf="@+id/textView16" />

    <RatingBar
        android:id="@+id/ratingBar6"
        android:layout_width="235dp"
        android:layout_height="45dp"
        android:layout_marginBottom="2dp"
        app:layout_constraintBottom_toTopOf="@+id/checkBox2"
        app:layout_constraintStart_toEndOf="@+id/textView23" />

    <RatingBar
        android:id="@+id/ratingBar7"
        android:layout_width="235dp"
        android:layout_height="45dp"
        android:layout_marginEnd="46dp"
        app:layout_constraintBottom_toTopOf="@+id/ratingBar6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView24"
        android:layout_marginRight="46dp" />

    <RatingBar
        android:id="@+id/ratingBar8"
        android:layout_width="235dp"
        android:layout_height="45dp"
        android:layout_marginEnd="48dp"
        app:layout_constraintBottom_toTopOf="@+id/ratingBar9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView26"
        android:layout_marginRight="48dp" />

    <RatingBar
        android:id="@+id/ratingBar9"
        android:layout_width="235dp"
        android:layout_height="45dp"
        android:layout_marginStart="2dp"
        app:layout_constraintBottom_toTopOf="@+id/ratingBar7"
        app:layout_constraintStart_toEndOf="@+id/textView25"
        android:layout_marginLeft="2dp" />

    <TextView
        android:id="@+id/textView27"
        android:layout_width="209dp"
        android:layout_height="43dp"
        android:layout_marginStart="51dp"
        android:layout_marginTop="13dp"
        android:text="Pregătire opțională la scoală :"
        android:gravity="center_vertical|center_horizontal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView23"
        android:layout_marginLeft="51dp" />

    <CheckBox
        android:id="@+id/checkBox2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="102dp"
        android:text="Da"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ratingBar6"
        android:layout_marginRight="102dp" />

    <CheckBox
        android:id="@+id/checkBox3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="101dp"
        android:text="Nu"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/checkBox2"
        android:layout_marginRight="101dp" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="322dp"
        android:layout_height="60dp"
        android:layout_marginStart="30dp"
        android:layout_marginBottom="80dp"
        android:ems="10"
        android:hint="Lasă un comentariu"
        android:gravity="center_vertical|center_horizontal"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/button2"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginLeft="30dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="219dp"
        android:layout_height="68dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/loginbutton"
        android:text="Lasa Recenzia!"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="155dp"
        android:layout_height="129dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="2dp"
        android:layout_marginBottom="67dp"
        app:layout_constraintBottom_toTopOf="@+id/textView16"
        app:layout_constraintEnd_toStartOf="@+id/textView11"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars"
        android:layout_marginLeft="50dp"
        android:layout_marginRight="2dp" />

</androidx.constraintlayout.widget.ConstraintLayout>